{
  "name": "mrbenosborne/json-unmarshal",
  "description": "A PHP 8 package for un-marshalling JSON data onto class properties.",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Ben Osborne",
      "email": "hello@benosborne.io",
      "role": "Creator"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "autoload": {
    "psr-4": {
      "JSON\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "vimeo/psalm": "^4.3",
    "psalm/plugin-phpunit": "^0.15.0"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit ./tests/ --testdox",
    "psalm": "./vendor/bin/psalm --no-cache --shepherd"
  }
}
